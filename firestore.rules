rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /notes/{noteId} {
      allow read, write: if true;
    }
    match /history/{historyId} {
      allow read, write: if true;
    }
    match /health_timers/{timerId} {
      allow read, write: if true;
    }
    match /health_history/{historyId} {
      allow read, write: if true;
    }
    match /motivational_speeches/{speechId} {
      allow read, write: if true;
    }
  }
}
